{
  "name": "frontend-testing-framework",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:nowarn": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix --cache",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "test": "jest",
    "test:unit": "jest --testMatch='**/*.unit.test.[jt]s?(x)'",
    "test:unit-watch": "jest --testMatch='**/*.unit.test.[jt]s?(x)' --watch",
    "test:unit-coverage": "jest --testMatch='**/*.unit.test.[jt]s?(x)' --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.[jt]s?(x)'",
    "test:integration-watch": "jest --testMatch='**/*.integration.test.[jt]s?(x)' --watch",
    "test:integration-coverage": "jest --testMatch='**/*.integration.test.[jt]s?(x)' --coverage",
    "test:crypress:open-all": "cypress open --config-file cypress.config.ts",
    "test:e2e": "cypress run --config-file cypress.config.ts --spec 'src/tests/e2e/**/*.e2e.cy.{ts,tsx}'",
    "test:e2e:open": "cypress open --config-file cypress.config.ts --spec 'src/tests/e2e/**/*.e2e.cy.{ts,tsx}'",
    "test:visual": "cypress run --config-file cypress.config.ts --spec 'src/tests/visual-tests/**/*.visual.cy.{ts,tsx}'",
    "test:visual:open": "cypress open --config-file cypress.config.ts --spec 'src/tests/visual-tests/**/*.visual.cy.{ts,tsx}'",
    "test:performance-lighthouse": "lighthouse http://localhost:3000 --output json --output html --view --output-path ./src/tests/performance-tests/lighthouse-report"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/mdx": "^2.0.13",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.3.0",
    "next": "14.2.23",
    "react": "^18",
    "react-dom": "^18",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "tailwind-merge": "^2.6.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/config-array": "^0.19.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@eslint/object-schema": "^2.1.5",
    "@types/node": "^20",
    "@types/react": "18.3.18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "cypress": "^14.0.3",
    "cypress-visual-regression": "^5.2.2",
    "eslint": "^9.15.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "postcss": "^8",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
